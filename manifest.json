{
    "manifest_version": 3,
    "name": "Tab Title Getter",
    "version": "1.0",
    "description": "Gets the current tab title and displays it in a popup.",
    "action":{
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    },
    "permissions":[
        "tabs"
    ]
}